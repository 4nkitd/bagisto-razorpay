{
    "name": "dagar/bagisto-razorpay",
    "description": "Razorpay Bagisto Integration",
    "type": "composer-plugin",
    "require": {
        "bagisto/bagisto": "dev-master",
        "razorpay/razorpay": "2.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ankit",
            "email": "ankit@dagar.in"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Bagisto\\Razorpay\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bagisto\\Razorpay\\RazorpayServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Bagisto\\Razorpay\\Facade"
            }
        }
    }
}
